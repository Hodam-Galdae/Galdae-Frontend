{
  "name": "Galdae",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "lint": "eslint .",
    "test": "jest",
    "cm": "cz",
    "android:dev": "react-native run-android --appId com.hodam.galdae.dev --mode devDebug",
    "android:dev-release": "react-native run-android --appId com.hodam.galdae.dev --mode devRelease",
    "android:prod": "react-native run-android --appId com.hodam.galdae --mode prodDebug",
    "android:prod-release": "react-native run-android --appId com.hodam.galdae --mode prodRelease",
    "android": "npm run android:dev",
    "android:release": "npm run android:prod-release",
    "ios": "cp ios/GoogleService-Info.dev.plist ios/Galdae/GoogleService-Info.plist && react-native run-ios --scheme Galdae-Dev",
    "ios:release": "cp ios/GoogleService-Info.prod.plist ios/Galdae/GoogleService-Info.plist && react-native run-ios --scheme Galdae-Prod --configuration Release",
    "build:android:dev": "cd android && ./gradlew assembleDevRelease",
    "build:android:prod": "cd android && ./gradlew assembleProdRelease",
    "build:android:bundle:dev": "cd android && ./gradlew bundleDevRelease",
    "build:android:bundle:prod": "cd android && ./gradlew bundleProdRelease",
    "clean:metro": "react-native start --reset-cache",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@gorhom/portal": "^1.0.14",
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-clipboard/clipboard": "^1.16.1",
    "@react-native-firebase/app": "^21.12.2",
    "@react-native-firebase/messaging": "^21.12.2",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-native-seoul/kakao-login": "^5.4.1",
    "@react-native-seoul/naver-login": "^4.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@stomp/stompjs": "^5.4.1",
    "axios": "^1.7.9",
    "axios-logger": "^2.8.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.47",
    "react": "18.3.1",
    "react-native": "^0.77.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.23.1",
    "react-native-image-picker": "^8.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modalize": "^2.1.1",
    "react-native-permissions": "^5.2.5",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.6.0",
    "react-native-svg": "^15.11.1",
    "react-native-video": "^6.10.2",
    "react-native-web-swiper": "^2.2.4",
    "react-native-webview": "^13.13.2",
    "react-redux": "^9.2.0",
    "redux-flipper": "^2.0.3",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/runtime": "^7.25.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
